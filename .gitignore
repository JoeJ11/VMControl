# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/.idea/*
ansible/roles/common/files/pub_key
ansible/roles/common/files/pri_key
ansible/roles/*