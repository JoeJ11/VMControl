<h1>Listing user_groups</h1>

<table>
  <thead>
    <tr>
      <th>Experiment</th>
      <th>Name</th>
      <th>Git token</th>
      <th>Git</th>
      <th>Public key</th>
      <th>Private key</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_groups.each do |user_group| %>
      <tr>
        <td><%= user_group.experiment %></td>
        <td><%= user_group.name %></td>
        <td><%= user_group.git_token %></td>
        <td><%= user_group.git_id %></td>
        <td><%= user_group.public_key %></td>
        <td><%= user_group.private_key %></td>
        <td><%= link_to 'Show', user_group %></td>
        <td><%= link_to 'Edit', edit_user_group_path(user_group) %></td>
        <td><%= link_to 'Destroy', user_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User group', new_user_group_path %>
